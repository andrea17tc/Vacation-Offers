<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ofertavacanta.controller.ClientsController">
    <children>
        <TableView fx:id="offersTableView" layoutX="21.0" layoutY="100.0" prefHeight="200.0" prefWidth="306.0">
            <columns>
                <TableColumn fx:id="hotelNameColumn" prefWidth="75.0" text="Hotel Name" />
                <TableColumn fx:id="locationNameColumn" prefWidth="75.0" text="Location Name" />
                <TableColumn fx:id="startDateColumn" prefWidth="75.0" text="Start Date" />
                <TableColumn fx:id="endDateColumn" prefWidth="75.0" text="End Date" />
            </columns>
        </TableView>
        <Button layoutX="21.0" layoutY="333.0" mnemonicParsing="false" onAction="#reserveButtonClicked" text="Search" />
    </children>
</AnchorPane>
